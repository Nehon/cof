 -2 en combat (déjà pris en compte dans ses Caractéristiques).</p><p><strong>Milieu : </strong><p>plaine</p>","attributes":{"hp":{"key":"hp","label":"COF.attributes.hp.label","abbrev":"COF.attributes.hp.abbrev","base":15,"bonus":0,"value":15,"max":15},"def":{"key":"def","label":"COF.attributes.def.label","abbrev":"COF.attributes.def.abbrev","base":13,"bonus":0,"value":13},"init":{"key":"init","label":"COF.attributes.init.label","abbrev":"COF.attributes.init.abbrev","base":10,"bonus":0,"value":10},"dr":{"key":"dr","label":"COF.attributes.dr.label","abbrev":"COF.attributes.dr.abbrev","enabled":false,"base":{"value":0,"ignored":""},"bonus":{"value":0,"ignored":""},"value":0},"rp":{"key":"rp","label":"COF.attributes.rp.label","abbrev":"COF.attributes.rp.abbrev","enabled":false,"base":5,"bonus":0,"value":5},"madness":{"key":"fp","label":"COF.attributes.madness.label","abbrev":"COF.attributes.madness.abbrev","enabled":false,"base":0,"value":0},"fp":{"key":"fp","label":"COF.attributes.fp.label","abbrev":"COF.attributes.fp.abbrev","enabled":false,"base":3,"bonus":0,"value":3,"max":3},"mp":{"key":"mp","label":"COF.attributes.mp.label","abbrev":"COF.attributes.mp.abbrev","enabled":false,"base":0,"bonus":0,"value":0,"max":0},"hd":{"key":"hd","label":"COF.attributes.hd.label","abbrev":"COF.attributes.hd.abbrev","value":"1d4"}},"currency":{"pp":{"key":"pp","label":"COF.currency.pp.label","abbrev":"COF.currency.pp.abbrev","qty":0},"gp":{"key":"gp","label":"COF.currency.gp.label","abbrev":"COF.currency.gp.abbrev","qty":0},"sp":{"key":"sp","label":"COF.currency.sp.label","abbrev":"COF.currency.sp.abbrev","qty":0},"cp":{"key":"cp","label":"COF.currency.cp.label","abbrev":"COF.currency.cp.abbrev","qty":0}},"alert":{"type":"error","msg":""},"details":{"gender":"","age":"","weight":"","height":"","eyes":"","hair":"","size":"big","senses":"","languages":{"value":[],"custom":""}},"key":"cheval-de-selle","nc":{"key":"nc","label":"COF.attributes.nc.label","abbrev":"COF.attributes.nc.abbrev